<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_properties" model="ir.sequence">
            <field name="name">Properties sequence</field>
            <field name="code">seq_properties</field>
            <field name="active">True</field>
            <field name="prefix">B</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_jobs" model="ir.sequence">
            <field name="name">Jobs sequence</field>
            <field name="code">seq_jobs</field>
            <field name="active">True</field>
            <field name="prefix">P</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_credit" model="ir.sequence">
            <field name="name">Credit liberations sequence</field>
            <field name="code">seq_credit</field>
            <field name="active">True</field>
            <field name="prefix">LIB</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>